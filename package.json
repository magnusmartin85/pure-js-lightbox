{
  "name": "lightbox",
  "version": "1.0.0",
  "description": "Simple jQuery Lightbox",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "step1": "rm -R public && mkdir public && cd public && touch .gitkeep && cd ..",
    "step2": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js && gulp",
    "build": "npm run step1 && npm run step2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magnusmartin85/lightbox.git"
  },
  "keywords": [
    "gallery",
    "images",
    "lightbox",
    "webpack"
  ],
  "author": "Magnus Martin <hello@mgnmrt.com>",
  "license": "ISC",
  "homepage": "lightbox.mgnmrt.com",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.13.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-image-resize": "^0.13.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
