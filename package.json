{
  "name": "lightbox",
  "version": "1.0.0",
  "description": "Simple jQuery Lightbox",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "step1": "rm -R public && mkdir public && cd public && touch .gitkeep && cd ",
    "step2": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js && gulp",
    "build": "npm run step1 && npm run step2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magnusmartin85/lightbox.git"
  },
  "keywords": [
    "gallery",
    "images",
    "lightbox",
    "webpack"
  ],
  "author": "Magnus Martin <hello@mgnmrt.com>",
  "license": "ISC",
  "homepage": "lightbox.mgnmrt.com",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "browser-sync": "^2.26.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-dependents": "^1.2.5",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylus": "^2.7.0",
    "gulp-uglify": "^3.0.2",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
  }
}
